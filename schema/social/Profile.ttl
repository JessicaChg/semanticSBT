PREFIX : <http://relationlabs.ai/entity/>
PREFIX p: <http://relationlabs.ai/property/>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>

###################
# class
###################
:Soul a rdfs:Class ;
    rdfs:label "soul" ;
    rdfs:comment "A soul" .

:Profile a rdfs:Class ;
    rdfs:label "profile" ;
    rdfs:comment "A Profile" .

:Contract a rdfs:Class ;
    rdfs:label "contract" ;
    rdfs:comment "A contract" .

###################
# predicate
###################

p:owner a rdfs:Class ;
    rdfs:label "owner" ;
    rdfs:comment "The owner" ;
    rdfs:range :Soul .

p:name a rdf:Property ;
    rdfs:label "name" ;
    rdfs:comment "The name" ;
    rdfs:range xsd:string .

p:avatar a rdf:Property ;
    rdfs:label "avatar" ;
    rdfs:comment "The avatar" ;
    rdfs:range xsd:string .

p:nostr a rdf:Property ;
    rdfs:label "nostr" ;
    rdfs:comment "The nostr" ;
    rdfs:range xsd:string .

p:connectionAddress a rdf:Property ;
    rdfs:label "connection" ;
    rdfs:comment "The address of connection contract" ;
    rdfs:range :Contract .